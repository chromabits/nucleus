{
  "name": "chromabits/nucleus",
  "description": "Some core PHP utilities",
  "license": "MIT",
  "authors": [
    {
      "name": "Eduardo Trujillo",
      "email": "ed@chromabits.com"
    }
  ],
  "require-dev": {
    "phpunit/phpunit": "*",
    "mockery/mockery": "0.9.*"
  },
  "autoload": {
    "psr-4": {
      "Chromabits\\Nucleus\\": "src/Chromabits/Nucleus"
    },
    "files": [
      "src/Chromabits/Nucleus/Support/globals.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\Chromabits\\Nucleus\\": "tests/Chromabits/Nucleus"
    }
  },
  "require": {
    "lstrojny/phpunit-function-mocker": "0.2.*"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.3-dev"
    }
  },
}
